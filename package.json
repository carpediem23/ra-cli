{
  "name": "@carpediem23/ra-cli",
  "version": "1.2.1",
  "description": "CLI tool to generate React component templates",
  "homepage": "https://github.com/carpediem23/ra-cli#readme",
  "bugs": {
    "url": "https://github.com/carpediem23/ra-cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carpediem23/ra-cli.git"
  },
  "license": "ISC",
  "author": "carpediem23 (https://github.com/carpediem23)",
  "type": "commonjs",
  "main": "src/index.ts",
  "keywords": [
    "react",
    "cli",
    "component",
    "generator",
    "template"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "ra": "bin/ra.js"
  },
  "files": [
    "dist",
    "bin",
    "templates"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "commander": "^8.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/node": "^16.0.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "^4.0.0",
    "typescript-eslint": "^8.28.0"
  }
}
